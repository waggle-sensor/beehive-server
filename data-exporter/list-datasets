from cassandra.cluster import Cluster
import logging

cluster = Cluster()
session = cluster.connect('waggle')

results = session.execute('SELECT DISTINCT node_id, date FROM sensor_data_raw')

for row in results:
    if not row.node_id:
        continue
    if not row.date:
        continue
    print(row.node_id, row.date)
